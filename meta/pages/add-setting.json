                            {
                              "component": "Used",
                              "name": "UsedLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Used",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "Used",
                              "type": "checkbox",
                              "value": "${setting.Used}",
                              "valueType": "bool"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "primary",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              },
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "type": "button"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "type": "row"
                    }
                  ],
                  "data": "${setting}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Setting!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "DM.createSetting",
                        "parameters": [
                          {
                            "name": "Setting",
                            "value": "${setting}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${setting}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Dm1.Models.Dm.Setting",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${setting != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "setting",
        "type": "set",
        "value": "new Dm1.Models.Dm.Setting(){}",
        "valueType": "Dm1.Models.Dm.Setting"
      }
    ]
  },
  "layout": "Main",
  "name": "Add Setting",
  "access": [
    "Authenticated"
  ]
}