                            {
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Description",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 40,
                              "name": "Description",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${setting.Description}",
                              "valueType": "string"
                            },
                            {
                              "component": "Description",
                              "name": "DescriptionRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "Description is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
